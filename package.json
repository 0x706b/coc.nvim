{
  "name": "coc.nvim",
  "version": "0.0.1",
  "description": "Completetion manage for neovim",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint -c tslint.json -p .",
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -p tsconfig.json --watch true --sourceMap",
    "test": "npm run build && jest --runInBand",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neoclide/coc.nvim.git"
  },
  "keywords": [
    "complete",
    "neovim"
  ],
  "author": "Qiming Zhao <chemzqm@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neoclide/coc.nvim/issues"
  },
  "homepage": "https://github.com/neoclide/coc.nvim#readme",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "json",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(src/__tests__/.*|(\\.|/)(test|spec))\\.ts$",
    "coverageDirectory": "./coverage/"
  },
  "devDependencies": {
    "@chemzqm/tsconfig": "0.0.2",
    "@chemzqm/tslint-config": "^1.0.14",
    "@types/debounce": "^1.0.0",
    "@types/diff": "^3.5.1",
    "@types/fast-diff": "^1.1.0",
    "@types/fb-watchman": "^2.0.0",
    "@types/fuzzaldrin": "^2.1.1",
    "@types/glob": "^5.0.35",
    "@types/jest": "^23.1.6",
    "@types/minimatch": "^3.0.3",
    "@types/node": "^10.5.2",
    "@types/opn": "^5.1.0",
    "@types/pify": "^3.0.2",
    "@types/semver": "^5.5.0",
    "@types/uuid": "^3.4.3",
    "@types/which": "^1.3.1",
    "jest": "^23.4.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.0.0"
  },
  "dependencies": {
    "@chemzqm/solargraph-utils": "0.7.5",
    "babel-polyfill": "^6.26.0",
    "cross-spawn": "^6.0.5",
    "debounce": "^1.1.0",
    "diff": "^3.5.0",
    "fast-diff": "^1.1.2",
    "fb-watchman": "^2.0.0",
    "fuzzaldrin": "^2.1.0",
    "glob": "^7.1.2",
    "html2plaintext": "^2.0.1",
    "https-proxy-agent": "^2.2.1",
    "jsonc-parser": "^2.0.1",
    "log4js": "^3.0.0",
    "matcher": "^1.1.1",
    "minimatch": "^3.0.4",
    "neovim": "^4.2.1",
    "node-serial": "^0.1.1",
    "opn": "^5.3.0",
    "pify": "^3.0.0",
    "semver": "^5.5.0",
    "shell-escape": "^0.2.0",
    "stylelint-vscode": "^7.0.0-6",
    "touch": "^3.1.0",
    "tslib": "^1.9.3",
    "tslint": "^5.10.0",
    "typescript": "^3.0.0-dev.20180710",
    "uuid": "^3.3.2",
    "vscode-css-languageservice": "^3.0.9-next.19",
    "vscode-html-languageserver-bin": "^1.4.0",
    "vscode-json-languageserver": "^1.0.1",
    "vscode-languageserver": "^4.3.0",
    "vscode-languageserver-protocol": "^3.9.0",
    "vscode-languageserver-types": "^3.9.0",
    "vscode-uri": "^1.0.5",
    "which": "^1.3.1",
    "wxml-langserver": "2.0.1"
  }
}
